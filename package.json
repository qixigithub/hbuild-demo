{
  "name": "page-core-xian",
  "description": "西安项目前端页面渲染引擎核心",
  "version": "1.0.25",
  "author": "author",
  "main": "dist/index.js",
  "license": "MIT",
  "private": false,
  "files": [
    "dist",
    "src",
    "themes"
  ],
  "scripts": {
    "new": "node build/bin/new.js",
    "new-nomd": "node build/bin/new-nomd.js",
    "json": "node build/bin/build-json.js&&npm run index",
    "index": "node build/bin/build-comps-index.js",
    "pages": "node build/bin/build-doc-pages.js",
    "page-tpl-watch": "node build/bin/template-watch.js",
    "dev": "npm run json&&cross-env NODE_ENV=development webpack-dev-server --open --hot --config ./build/config/webpack.dev.config.js",
    "build": "rimraf ./dist && npm run json && cross-env NODE_ENV=production webpack --config ./build/config/webpack.dist.config.js --progress --hide-modules",
    "dist": "npm run clean && npm run json && npm run dist:cmd && npm run dist:umd && npm run dist:theme",
    "dist:component": "npm run clean && npm run json && npm run dist:cmd && npm run dist:umd",
    "dist:cmd": "cross-env NODE_ENV=production webpack --config ./build/config/webpack.dist.cmd.config.js --progress --hide-modules",
    "dist:umd": "cross-env NODE_ENV=production webpack --config ./build/config/webpack.dist.umd.config.js --progress --hide-modules",
    "dist:all": "npm run clean && lerna run dist:component",
    "pub": "npm run dist && npm publish",
    "pub:component": "npm run dist:component && npm publish",
    "pub:all": "npm run dist:all && lerna publish --skip-git",
    "clean": "rimraf dist && rimraf src/components/*/dist && npm run clean:theme",
    "theme": "node build/bin/theme.js",
    "dist:theme": "lerna run dist:theme",
    "clean:theme": "rimraf themes/*/dist && lerna clean --yes",
    "gen-search": "node build/bin/gen-indices.js"
  },
  "dependencies": {
    "accounting": "^0.4.1",
    "element-ui": "^2.2.0",
    "iview": "^3.1.5",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "v-viewer": "^1.2.1",
    "vue": "^2.5.11",
    "vue-loader": "^13.3.0",
    "vuex": "^3.0.1",
    "plupload": "^2.3.6",
    "algoliasearch": "^3.24.11",
    "vue-router": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^2.0.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "file-save": "^0.2.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-sass": "^3.1.0",
    "highlight.js": "^9.12.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "json-templater": "^1.2.0",
    "lerna": "^2.9.0",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-container": "^2.0.0",
    "node-sass": "^4.7.2",
    "perspective.js": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "stylus-loader": "^3.0.2",
    "throttle-debounce": "^1.0.1",
    "transliteration": "^1.6.2",
    "uppercamelcase": "^3.0.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.3.0",
    "vue-markdown-loader": "^2.3.0",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.2"
  }
}
